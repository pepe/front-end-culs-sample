<script>
	var color;

	function changeSkin(color){
		$("link#skin-css").attr("href","assets/css/colors/"+color);
	}

	function changeLayout(layout_skin){
		$("link#layout-color").attr("href","assets/css/"+layout_skin);
		return false;
	}

	function changeNav(iswhite){
		if(!iswhite){
			$("header").addClass('dark');
		}
		else{
			$("header").removeClass('dark');
		}
		return false;
	}

	function naviStick(){
		var state = $(this).data('toggleState');
		if (state) {
			$("#sticktop").removeClass('nav-stop');
			$('.switch-icon').removeClass('right-switch');
		} else {
			$("#sticktop").addClass('nav-stop');
			$('.switch-icon').addClass('right-switch');
		}
		$(this).data('toggleState', !state);
	}

	$('.toggler').click(function(){

		var state = $(this).data('toggleState');
        if(state) {
           $('#switcher').css('left','-240px');
        } else {
            $('#switcher').css('left','-10px');
        }
        $(this).data('toggleState', !state);

	});


</script>


